      <!-- toc -->
      <div id="toc">
        <ul>
          <% unless @object.documentation.summary.empty? %>
          <li><a href="#" title="Foo">Overview</a></li>
          <% end %>
          
          <% tasks = tasks_for_object(@object) %>        
          <% if tasks.any? %>
          <li>
            <a href="#" title="Tasks">Tasks</a>
            <ul>
            <% tasks.each do |key, members| %>
              <li><%= link_to_task(key, @object) %></li>
            <% end %>
            </ul>
          </li>
          <% end %>
          <% if @object.properties.any? %>
          <li>
            <a href="#<%= anchor_for_collection(@object.properties) %>" title="Foo">Properties</a>
            <ul>
              <% @object.properties.each do |property| %>
              <li><a href="#<%= property.ref %>" title=""><%= property.name %></a></li>
              <% end %>
            </ul>
          </li>
          <% end %>
          
          <% if @object.instance_methods.any? %>
          <li>
            <a href="#<%= anchor_for_collection(@object.instance_methods) %>" title="Foo">Instance Methods</a>
            <ul>
              <% @object.instance_methods.each do |method| %>
              <li><a href="#<%= method.ref %>" title=""><%= method.name %></a></li>
              <% end %>
            </ul>
          </li>
          <% end %>
    
          <% if @object.class_methods.any? %>
          <li>
            <a href="#<%= anchor_for_collection(@object.class_methods) %>" title="Foo">Class Methods</a>
            <ul>
              <% @object.class_methods.each do |method| %>
              <li><a href="#<%= method.ref %>" title=""><%= method.name %></a></li>
              <% end %>
            </ul>
          </li>
          <% end %>
        </ul>
      </div>
      <!-- /toc -->